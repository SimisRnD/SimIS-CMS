<template name="Multiple Content Blocks" image="Multiple Content Blocks.png">
  <page>
    <section>
      <column class="small-12 medium-6 cell">
        <widget name="content">
          <uniqueId>${webPageName}-hello</uniqueId>
        </widget>
      </column>
      <column class="small-6 medium-3 callout box cell">
        <widget name="content">
          <uniqueId>${webPageName}-callout-1</uniqueId>
        </widget>
      </column>
      <column class="small-6 medium-3 callout secondary cell">
        <widget name="content">
          <uniqueId>${webPageName}-callout-2</uniqueId>
        </widget>
      </column>
    </section>
    <section hr="true">
      <column class="small-12 medium-4 cell">
        <widget name="content">
          <uniqueId>${webPageName}-benefits-1</uniqueId>
        </widget>
      </column>
      <column class="small-12 medium-4 cell">
        <widget name="content">
          <uniqueId>${webPageName}-benefits-2</uniqueId>
        </widget>
      </column>
      <column class="small-12 medium-4 cell">
        <widget name="content">
          <uniqueId>${webPageName}-benefits-3</uniqueId>
        </widget>
      </column>
    </section>
  </page>
</template>
